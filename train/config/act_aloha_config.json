{
    "dataset": {
        "repo_id": "lerobot/aloha_sim_insertion_human",
        "video_backend": "pyav",
        "revision": "false"
    },
    "env": {
        "type": "aloha",
        "task": "AlohaInsertion-v0",
        "fps": 50,
        "episode_length": 400,
        "obs_type": "pixels_agent_pos",
        "render_mode": "rgb_array"
    },
    "policy": {
        "type": "act",
        "device": "cuda",
        "use_amp": "true",
        "use_compile": "true",
        "compile_mode": "reduce-overhead",
        "compile_fullgraph": "false",
        "n_obs_steps": 1,
        "chunk_size": 100,
        "n_action_steps": 100,
        "vision_backbone": "resnet18",
        "pretrained_backbone_weights": "ResNet18_Weights.IMAGENET1K_V1",
        "pre_norm": "false",
        "dim_model": 512,
        "n_heads": 8,
        "dim_feedforward": 3200,
        "feedforward_activation": "relu",
        "n_encoder_layers": 4,
        "n_decoder_layers": 1,
        "use_vae": "true",
        "latent_dim": 32,
        "n_vae_encoder_layers": 4,
        "dropout": 0.1,
        "kl_weight": 10.0,
        "optimizer_lr": 1e-5,
        "optimizer_weight_decay": 1e-4,
        "optimizer_lr_backbone": 1e-5
    },

    "resume": "false",
    "seed": 1000,
    "num_gpus": 1,
    "num_workers": 4,
    "batch_size": 8,
    "steps": 100000,
    "eval_freq": 20000,
    "log_freq": 200,
    "save_checkpoint": "true",
    "save_freq": 25000,
    "use_policy_training_preset": "true",
    "eval": {
        "n_episodes": 10,
        "batch_size": 4
    },
    "wandb": {
        "enable": "false",
        "disable_artifact": "false",
        "project": "lerobot"
    }
}